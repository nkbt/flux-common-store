{
  "name": "flux-common-store",
  "version": "1.0.1",
  "description": "Common Flux Store",
  "main": "lib/FluxCommonStore.js",
  "scripts": {
    "start": "npm run build:dev",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rm -rf lib",
    "build:lib": "babel src --source-maps --out-dir lib",
    "build:dev": "babel src --source-maps --out-dir lib --watch",
    "prepublish": "npm run build",
    "lint": "eslint .",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nkbt/flux-common-store.git"
  },
  "keywords": [
    "component",
    "react-component",
    "react",
    "flux",
    "flux-component",
    "store",
    "flux-store"
  ],
  "author": "Nik Butenko <nik@butenko.me> (http://butenko.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkbt/flux-common-store/issues"
  },
  "homepage": "https://github.com/nkbt/flux-common-store",
  "dependencies": {
    "lodash.throttle": "^3.0.4"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.20",
    "babel-plugin-object-assign": "^1.2.0",
    "eslint": "^0.24.1"
  }
}
